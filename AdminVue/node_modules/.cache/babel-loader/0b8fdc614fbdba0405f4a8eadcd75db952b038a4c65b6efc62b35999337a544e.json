{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { mixin } from \"../../../minix\";\nimport { getUserById, editUser, getCityList, getApeProvinceList, getApeBusinessList } from '../../../api/api';\nexport default {\n  mixins: [mixin],\n  data() {\n    return {\n      province: [],\n      city: [],\n      business: [],\n      cityFilter: [],\n      businessFilter: [],\n      formLabelWidth: '80px',\n      form: {\n        idArrary: \"\",\n        deptId: \"\",\n        userName: \"\",\n        loginAccount: \"\",\n        userType: 1,\n        email: \"\",\n        tel: \"\",\n        sex: \"\",\n        avatar: \"\",\n        password: \"\",\n        status: \"0\",\n        remark: \"\",\n        roleIds: [],\n        coverImg: \"\",\n        name: \"\",\n        star: \"\",\n        openTime: \"\",\n        address: \"\",\n        average: \"\",\n        province: \"\",\n        city: \"\",\n        business: \"\",\n        type: \"线下\",\n        open: \"0\",\n        introduce: \"\"\n      },\n      type: [\"线下\"],\n      fileList: [],\n      dialogImageUrl: '',\n      dialogVisible: false,\n      rules: {\n        userName: [{\n          required: true,\n          message: '请输入用户昵称',\n          trigger: 'blur'\n        }],\n        loginAccount: [{\n          required: true,\n          message: '请输入登陆账号',\n          trigger: 'blur'\n        }],\n        password: [{\n          required: true,\n          message: '请输入登陆密码',\n          trigger: 'blur'\n        }],\n        coverImg: [{\n          required: true,\n          message: '请上传商家封面图片',\n          trigger: 'blur'\n        }],\n        name: [{\n          required: true,\n          message: '请输入店铺名称',\n          trigger: 'blur'\n        }],\n        star: [{\n          required: true,\n          message: '请输入评分',\n          trigger: 'blur'\n        }],\n        openTime: [{\n          required: true,\n          message: '请输入营业时间',\n          trigger: 'blur'\n        }],\n        address: [{\n          required: true,\n          message: '请输入地址',\n          trigger: 'blur'\n        }],\n        average: [{\n          required: true,\n          message: '请输入人均消费',\n          trigger: 'blur'\n        }],\n        province: [{\n          required: true,\n          message: '请选择省份',\n          trigger: 'blur'\n        }],\n        city: [{\n          required: true,\n          message: '请选择城市',\n          trigger: 'blur'\n        }],\n        business: [{\n          required: true,\n          message: '请选择商圈',\n          trigger: 'blur'\n        }],\n        introduce: [{\n          required: true,\n          message: '请输入商家简介',\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n  props: ['updateUserVisible', 'updateId'],\n  methods: {\n    handleAvatorSuccess(res) {\n      let _this = this;\n      if (res.code == 1000) {\n        _this.$message({\n          type: 'success',\n          message: '上传成功!'\n        });\n        this.fileList.push({\n          url: this.$store.state.configure.HOST + res.message\n        });\n        this.form.coverImg = res.message;\n      } else {\n        _this.$notify.error({\n          title: '错误',\n          message: res.message\n        });\n      }\n    },\n    handleRemove(file, fileList) {\n      this.form.coverImg = \"\";\n      this.fileList = [];\n    },\n    handlePictureCardPreview(file) {\n      this.dialogImageUrl = file.url;\n      this.dialogVisible = true;\n    },\n    getApeProvinceList() {\n      getApeProvinceList().then(res => {\n        if (res.code == 1000) {\n          this.province = res.data;\n        }\n      });\n    },\n    getCityList() {\n      getCityList().then(res => {\n        if (res.code == 1000) {\n          this.city = res.data;\n          this.changeProvince(this.form.province);\n        }\n      });\n    },\n    getApeBusinessList() {\n      getApeBusinessList().then(res => {\n        if (res.code == 1000) {\n          this.business = res.data;\n          this.changeCity(this.form.city);\n        }\n      });\n    },\n    changeProvince(val) {\n      this.cityFilter = [];\n      for (let i = 0; i < this.city.length; i++) {\n        let item = this.city[i];\n        if (item.province == val) {\n          this.cityFilter.push(item);\n        }\n      }\n    },\n    changeCity(val) {\n      this.businessFilter = [];\n      for (let i = 0; i < this.business.length; i++) {\n        let item = this.business[i];\n        if (item.city == val) {\n          this.businessFilter.push(item);\n        }\n      }\n    },\n    submit() {\n      this.$refs[\"ruleForm\"].validate(valid => {\n        if (valid) {\n          this.form.type = this.type.join(\",\");\n          editUser(this.form).then(res => {\n            if (res.code == 1000) {\n              this.$notify.success({\n                title: '成功',\n                message: \"保存成功\"\n              });\n              this.handleClose();\n            } else {\n              this.$notify.error({\n                title: '错误',\n                message: res.message\n              });\n            }\n          });\n        }\n      });\n    },\n    handleClose() {\n      this.fileList = [];\n      this.$emit(\"updateUserFalse\");\n    }\n  },\n  mounted() {\n    this.getApeProvinceList();\n  },\n  watch: {\n    updateId(newVal) {\n      if (newVal) {\n        getUserById({\n          id: newVal\n        }).then(res => {\n          if (res.code == 1000) {\n            this.form = res.data;\n            this.form.sex = res.data.sex + \"\";\n            this.form.status = res.data.status + \"\";\n            this.form.open = res.data.open + \"\";\n            this.type = res.data.type.split(\",\");\n            this.fileList.push({\n              url: this.$store.state.configure.HOST + this.form.coverImg\n            });\n            this.getCityList();\n            this.getApeBusinessList();\n          } else {\n            this.$notify.error({\n              title: '错误',\n              message: res.message\n            });\n          }\n        });\n      }\n    }\n  }\n};","map":{"version":3,"names":["mixin","getUserById","editUser","getCityList","getApeProvinceList","getApeBusinessList","mixins","data","province","city","business","cityFilter","businessFilter","formLabelWidth","form","idArrary","deptId","userName","loginAccount","userType","email","tel","sex","avatar","password","status","remark","roleIds","coverImg","name","star","openTime","address","average","type","open","introduce","fileList","dialogImageUrl","dialogVisible","rules","required","message","trigger","props","methods","handleAvatorSuccess","res","_this","code","$message","push","url","$store","state","configure","HOST","$notify","error","title","handleRemove","file","handlePictureCardPreview","then","changeProvince","changeCity","val","i","length","item","submit","$refs","validate","valid","join","success","handleClose","$emit","mounted","watch","updateId","newVal","id","split"],"sources":["src/components/system/shop/updateShop.vue"],"sourcesContent":["<template>\r\n<div>\r\n  <el-dialog title=\"修改用户\" width=\"40%\" :destroy-on-close=\"true\" :visible.sync=\"updateUserVisible\" :before-close=\"handleClose\">\r\n    <el-form :model=\"form\" :rules=\"rules\" ref=\"ruleForm\">\r\n      <el-row :gutter=\"10\">\r\n        <el-col :xs=\"24\" :sm=\"24\" :md=\"24\" :lg=\"24\" :xl=\"24\">\r\n            <span class=\"search-title\">商家昵称:</span>\r\n            <div style=\"width:100%\">\r\n                <el-form-item prop=\"userName\" style=\"margin-bottom:0\">\r\n                    <el-input v-model=\"form.userName\" size=\"mini\" placeholder=\"请输入用户昵称\" autocomplete=\"off\"></el-input>\r\n                </el-form-item>\r\n            </div>\r\n        </el-col>\r\n        <el-col :xs=\"24\" :sm=\"24\" :md=\"24\" :lg=\"24\" :xl=\"24\">\r\n            <span class=\"search-title\">手机号码:</span>\r\n            <div style=\"width:100%\">\r\n                <el-form-item prop=\"tel\" style=\"margin-bottom:0\">\r\n                    <el-input v-model=\"form.tel\" size=\"mini\" placeholder=\"请输入手机号码\" autocomplete=\"off\"></el-input>\r\n                </el-form-item>\r\n            </div>\r\n        </el-col>\r\n        <el-col :xs=\"24\" :sm=\"24\" :md=\"24\" :lg=\"24\" :xl=\"24\">\r\n            <span class=\"search-title\">&nbsp;&nbsp;&nbsp;&nbsp;邮箱:</span>\r\n            <div style=\"width:100%\">\r\n                <el-form-item prop=\"email\" style=\"margin-bottom:0\">\r\n                    <el-input v-model=\"form.email\" size=\"mini\" placeholder=\"请输入邮箱\" autocomplete=\"off\"></el-input>\r\n                </el-form-item>\r\n            </div>\r\n        </el-col>\r\n        <el-col :xs=\"24\" :sm=\"24\" :md=\"24\" :lg=\"24\" :xl=\"24\">\r\n            <span class=\"search-title\">登陆账号:</span>\r\n            <div style=\"width:100%\">\r\n                <el-form-item prop=\"loginAccount\" style=\"margin-bottom:0\">\r\n                    <el-input v-model=\"form.loginAccount\" size=\"mini\" placeholder=\"请输入登陆账号\" autocomplete=\"off\"></el-input>\r\n                </el-form-item>\r\n            </div>\r\n        </el-col>\r\n        <el-col :xs=\"24\" :sm=\"24\" :md=\"24\" :lg=\"24\" :xl=\"24\">\r\n            <span class=\"search-title\">&nbsp;&nbsp;&nbsp;&nbsp;性别:</span>\r\n            <div style=\"width:100%\">\r\n                <el-form-item prop=\"sex\" style=\"margin-bottom:0\">\r\n                    <el-select size=\"mini\" v-model=\"form.sex\" placeholder=\"请选择用户性别\">\r\n                        <el-option label=\"男\" value=\"0\"></el-option>\r\n                        <el-option label=\"女\" value=\"1\"></el-option>\r\n                    </el-select>\r\n                </el-form-item>\r\n            </div>\r\n        </el-col>\r\n        <el-col :xs=\"24\" :sm=\"24\" :md=\"24\" :lg=\"24\" :xl=\"24\">\r\n            <span class=\"search-title\">封面图片:</span>\r\n            <div style=\"width:100%\">\r\n                <el-form-item prop=\"coverImg\" style=\"margin-bottom:0\">\r\n                    <el-upload\r\n                        :file-list=\"fileList\"\r\n                        ref=\"upload\"\r\n                        :action=\"uploadImageUrl()\"\r\n                        accept=\"image/*\"\r\n                        :multiple=\"false\"\r\n                        :limit=\"1\"\r\n                        :before-upload=\"beforeAvatorUpload\"\r\n                        list-type=\"picture-card\"\r\n                        :on-preview=\"handlePictureCardPreview\"\r\n                        :on-exceed=\"handleExceed\"\r\n                        :on-success=\"handleAvatorSuccess\"\r\n                        :on-remove=\"handleRemove\">\r\n                        <i class=\"el-icon-plus\"></i>\r\n                    </el-upload>\r\n                    <el-dialog title=\"预览\" :visible.sync=\"dialogVisible\">\r\n                        <img width=\"100%\" :src=\"dialogImageUrl\" alt=\"\">\r\n                    </el-dialog>\r\n                </el-form-item>\r\n            </div>\r\n        </el-col>\r\n        <el-col :xs=\"24\" :sm=\"24\" :md=\"24\" :lg=\"24\" :xl=\"24\">\r\n            <span class=\"search-title\">店铺名称:</span>\r\n            <div style=\"width:100%\">\r\n                <el-form-item prop=\"name\" style=\"margin-bottom:0\">\r\n                    <el-input v-model=\"form.name\" size=\"mini\" placeholder=\"请输入店铺名称\" autocomplete=\"off\"></el-input>\r\n                </el-form-item>\r\n            </div>\r\n        </el-col>\r\n        <el-col :xs=\"24\" :sm=\"24\" :md=\"24\" :lg=\"24\" :xl=\"24\">\r\n            <span class=\"search-title\">&nbsp;&nbsp;&nbsp;&nbsp;评分</span>:</span>\r\n            <div style=\"width:100%\">\r\n                <el-form-item prop=\"star\" style=\"margin-bottom:0\">\r\n                    <el-input v-model=\"form.star\" type=\"number\" min=\"0\" size=\"mini\" placeholder=\"请输入店铺评分\" autocomplete=\"off\"></el-input>\r\n                </el-form-item>\r\n            </div>\r\n        </el-col>\r\n        <el-col :xs=\"24\" :sm=\"24\" :md=\"24\" :lg=\"24\" :xl=\"24\">\r\n            <span class=\"search-title\">营业时间:</span>\r\n            <div style=\"width:100%\">\r\n                <el-form-item prop=\"openTime\" style=\"margin-bottom:0\">\r\n                    <el-input v-model=\"form.openTime\" size=\"mini\" placeholder=\"请输入营业时间\" autocomplete=\"off\"></el-input>\r\n                </el-form-item>\r\n            </div>\r\n        </el-col>\r\n        <el-col :xs=\"24\" :sm=\"24\" :md=\"24\" :lg=\"24\" :xl=\"24\">\r\n            <span class=\"search-title\">&nbsp;&nbsp;&nbsp;&nbsp;地址:</span>\r\n            <div style=\"width:100%\">\r\n                <el-form-item prop=\"address\" style=\"margin-bottom:0\">\r\n                    <el-input v-model=\"form.address\" size=\"mini\" placeholder=\"请输入地址\" autocomplete=\"off\"></el-input>\r\n                </el-form-item>\r\n            </div>\r\n        </el-col>\r\n        <el-col :xs=\"24\" :sm=\"24\" :md=\"24\" :lg=\"24\" :xl=\"24\">\r\n            <span class=\"search-title\">人均消费:</span>\r\n            <div style=\"width:100%\">\r\n                <el-form-item prop=\"average\" style=\"margin-bottom:0\">\r\n                    <el-input v-model=\"form.average\" size=\"mini\" placeholder=\"请输入人均消费\" autocomplete=\"off\"></el-input>\r\n                </el-form-item>\r\n            </div>\r\n        </el-col>\r\n        <el-col :xs=\"24\" :sm=\"24\" :md=\"24\" :lg=\"24\" :xl=\"24\">\r\n            <span class=\"search-title\">&nbsp;&nbsp;&nbsp;&nbsp;省份:</span>\r\n            <div style=\"width:100%\">\r\n                <el-form-item prop=\"province\" style=\"margin-bottom:0\">\r\n                    <el-select @change=\"changeProvince\" size=\"mini\" clearable v-model=\"form.province\" placeholder=\"请选择省份\">\r\n                        <el-option v-for=\"(item,index) in province\" :key=\"index\"\r\n                            :label=\"item.name\"\r\n                            :value=\"item.name\">\r\n                        </el-option>\r\n                    </el-select>\r\n                </el-form-item>\r\n            </div>\r\n        </el-col>\r\n        <el-col :xs=\"24\" :sm=\"24\" :md=\"24\" :lg=\"24\" :xl=\"24\">\r\n            <span class=\"search-title\">&nbsp;&nbsp;&nbsp;&nbsp;城市:</span>\r\n            <div style=\"width:100%\">\r\n                <el-form-item prop=\"city\" style=\"margin-bottom:0\">\r\n                    <el-select @change=\"changeCity\" size=\"mini\" clearable v-model=\"form.city\" placeholder=\"请选择城市\">\r\n                        <el-option v-for=\"(item,index) in cityFilter\" :key=\"index\"\r\n                            :label=\"item.city\"\r\n                            :value=\"item.city\">\r\n                        </el-option>\r\n                    </el-select>\r\n                </el-form-item>\r\n            </div>\r\n        </el-col>\r\n        <el-col :xs=\"24\" :sm=\"24\" :md=\"24\" :lg=\"24\" :xl=\"24\">\r\n            <span class=\"search-title\">&nbsp;&nbsp;&nbsp;&nbsp;商圈:</span>\r\n            <div style=\"width:100%\">\r\n                <el-form-item prop=\"business\" style=\"margin-bottom:0\">\r\n                    <el-select size=\"mini\" clearable v-model=\"form.business\" placeholder=\"请选择商圈\">\r\n                        <el-option v-for=\"(item,index) in businessFilter\" :key=\"index\"\r\n                            :label=\"item.business\"\r\n                            :value=\"item.business\">\r\n                        </el-option>\r\n                    </el-select>\r\n                </el-form-item>\r\n            </div>\r\n        </el-col>\r\n        <el-col style=\"margin-top:20px\" :xs=\"24\" :sm=\"24\" :md=\"24\" :lg=\"24\" :xl=\"24\">\r\n            <span class=\"search-title\">&nbsp;&nbsp;&nbsp;&nbsp;简介:</span>\r\n            <div style=\"width:100%\">\r\n                <el-form-item prop=\"introduce\" style=\"margin-bottom:0\">\r\n                    <el-input type=\"textarea\" rows=\"5\" v-model=\"form.introduce\"></el-input>\r\n                </el-form-item>\r\n            </div>\r\n        </el-col>\r\n        <el-col style=\"margin-top:20px\" :xs=\"24\" :sm=\"24\" :md=\"24\" :lg=\"24\" :xl=\"24\">\r\n            <span class=\"search-title\">购买方式:</span>\r\n            <div style=\"width:100%\">\r\n                <el-form-item prop=\"type\" style=\"margin-bottom:0\">\r\n                    <el-checkbox-group v-model=\"type\">\r\n                        <el-checkbox label=\"线下\" disabled></el-checkbox>\r\n                        <el-checkbox label=\"外卖\"></el-checkbox>\r\n                    </el-checkbox-group>\r\n                </el-form-item>\r\n            </div>\r\n        </el-col>\r\n        <el-col style=\"margin-top:20px\" :xs=\"24\" :sm=\"24\" :md=\"24\" :lg=\"24\" :xl=\"24\">\r\n            <span class=\"search-title\">是否营业:</span>\r\n            <div style=\"width:100%\">\r\n                <el-form-item prop=\"open\" style=\"margin-bottom:0\">\r\n                    <el-radio-group style=\"margin-top:1px;height:40px;display:flex;align-items: center\" v-model=\"form.open\">\r\n                        <el-radio label=\"0\">打样</el-radio>\r\n                        <el-radio label=\"1\">营业中</el-radio>\r\n                    </el-radio-group>\r\n                </el-form-item>\r\n            </div>\r\n        </el-col>\r\n        <el-col :xs=\"24\" :sm=\"24\" :md=\"24\" :lg=\"24\" :xl=\"24\">\r\n            <span class=\"search-title\">&nbsp;&nbsp;&nbsp;&nbsp;状态:</span>\r\n            <div style=\"width:100%\">\r\n                <el-form-item prop=\"status\" style=\"margin-bottom:0\">\r\n                    <el-radio-group style=\"margin-top:1px;height:40px;display:flex;align-items: center\" v-model=\"form.status\">\r\n                        <el-radio label=\"0\">正常</el-radio>\r\n                        <el-radio label=\"1\">停用</el-radio>\r\n                    </el-radio-group>\r\n                </el-form-item>\r\n            </div>\r\n        </el-col>\r\n        <el-col :xs=\"24\" :sm=\"24\" :md=\"24\" :lg=\"24\" :xl=\"24\">\r\n            <span class=\"search-title\">&nbsp;&nbsp;&nbsp;&nbsp;备注:</span>\r\n            <el-input type=\"textarea\" v-model=\"form.remark\"></el-input>\r\n        </el-col>\r\n      </el-row>\r\n    </el-form>\r\n    <div slot=\"footer\" class=\"dialog-footer\">\r\n      <el-button size=\"mini\" type=\"primary\" @click=\"submit\">确 定</el-button>\r\n      <el-button size=\"mini\" @click=\"handleClose\">取 消</el-button>\r\n    </div>\r\n  </el-dialog>\r\n</div>\r\n</template>\r\n\r\n<script>\r\n  import {mixin} from \"../../../minix\";\r\n  import {getUserById,editUser,getCityList,getApeProvinceList,getApeBusinessList} from '../../../api/api'\r\n  export default {\r\n    mixins: [mixin],\r\n    data() {\r\n      return{\r\n        province: [],\r\n        city: [],\r\n        business: [],\r\n        cityFilter: [],\r\n        businessFilter: [],\r\n        formLabelWidth: '80px',\r\n        form: {\r\n          idArrary: \"\",\r\n          deptId: \"\",\r\n          userName: \"\",\r\n          loginAccount: \"\",\r\n          userType: 1,\r\n          email: \"\",\r\n          tel: \"\",\r\n          sex: \"\",\r\n          avatar: \"\",\r\n          password: \"\",\r\n          status: \"0\",\r\n          remark: \"\",\r\n          roleIds: [],\r\n          coverImg: \"\",\r\n          name: \"\",\r\n          star: \"\",\r\n          openTime: \"\",\r\n          address: \"\",\r\n          average: \"\",\r\n          province: \"\",\r\n          city: \"\",\r\n          business: \"\",\r\n          type: \"线下\",\r\n          open: \"0\",\r\n          introduce: \"\",\r\n        },\r\n        type: [\"线下\"],\r\n        fileList: [],\r\n        dialogImageUrl: '',\r\n        dialogVisible: false,\r\n        rules: {\r\n          userName: [\r\n            { required: true, message: '请输入用户昵称', trigger: 'blur' },\r\n       