eholder=\"请输入角色名称\"\r\n                        suffix-icon=\"el-icon-date\"\r\n                        v-model=\"search.rolename\">\r\n                    </el-input>\r\n                </el-col>\r\n                <el-col :xs=\"24\" :sm=\"12\" :md=\"8\" :lg=\"8\" :xl=\"8\">\r\n                    <span class=\"search-title\">权限字符:</span>\r\n                    <el-input\r\n                        style=\"margin-top:10px\"\r\n                        size=\"mini\"\r\n                        placeholder=\"请输入权限字符\"\r\n                        suffix-icon=\"el-icon-date\"\r\n                        v-model=\"search.permission\">\r\n                    </el-input>\r\n                </el-col>\r\n                <el-col :xs=\"24\" :sm=\"12\" :md=\"8\" :lg=\"8\" :xl=\"8\">\r\n                    <span class=\"search-title\">角色状态:</span>\r\n                    <el-select style=\"margin-top:10px\" size=\"mini\" v-model=\"search.state\" placeholder=\"请选择\">\r\n                      <el-option\r\n                        label=\"正常\"\r\n                        value=\"0\">\r\n                      </el-option>\r\n                      <el-option\r\n                        label=\"停用\"\r\n                        value=\"1\">\r\n                      </el-option>\r\n                    </el-select>\r\n                </el-col>\r\n                <el-col :xs=\"24\" :sm=\"12\" :md=\"8\" :lg=\"8\" :xl=\"8\">\r\n                    <span class=\"search-title\">创建时间:</span>\r\n                    <el-date-picker size=\"mini\"\r\n                      style=\"margin-top:10px\"\r\n                      v-model=\"search.startTime\"\r\n                      type=\"daterange\"\r\n                      range-separator=\"至\"\r\n                      start-placeholder=\"开始日期\"\r\n                      end-placeholder=\"结束日期\">\r\n                    </el-date-picker>\r\n                </el-col>\r\n                <el-col :xs=\"24\" :sm=\"12\" :md=\"8\" :lg=\"8\" :xl=\"8\">\r\n               