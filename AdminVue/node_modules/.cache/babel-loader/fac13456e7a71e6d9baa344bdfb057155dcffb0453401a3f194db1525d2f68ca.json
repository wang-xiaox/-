[{\n          required: true,\n          message: '请输入地址',\n          trigger: 'blur'\n        }],\n        average: [{\n          required: true,\n          message: '请输入人均消费',\n          trigger: 'blur'\n        }],\n        province: [{\n          required: true,\n          message: '请选择省份',\n          trigger: 'blur'\n        }],\n        city: [{\n          required: true,\n          message: '请选择城市',\n          trigger: 'blur'\n        }],\n        business: [{\n          required: true,\n          message: '请选择商圈',\n          trigger: 'blur'\n        }],\n        introduce: [{\n          required: true,\n          message: '请输入商家简介',\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n  props: ['addUserVisible'],\n  methods: {\n    handleAvatorSuccess(res) {\n      let _this = this;\n      if (res.code == 1000) {\n        _this.$message({\n          type: 'success',\n          message: '上传成功!'\n        });\n        this.fileList.push({\n          url: this.$store.state.configure.HOST + res.message\n        });\n        this.form.coverImg = res.message;\n      } else {\n        _this.$notify.error({\n          title: '错误',\n          message: res.message\n        });\n      }\n    },\n    handleRemove(file, fileList) {\n      this.form.coverImg = \"\";\n      this.fileList = [];\n    },\n    handlePictureCardPreview(file) {\n      this.dialogImageUrl = file.url;\n      this.dialogVisible = true;\n    },\n    getApeProvinceList() {\n      getApeProvinceList().then(res => {\n        if (res.code == 1000) {\n          this.province = res.data;\n        }\n      });\n    },\n    getCityList() {\n      getCityList().then(res => {\n        if (res.code == 1000) {\n          this.city = res.data;\n        }\n      });\n    },\n    getApeBusinessList() {\n      getApeBusinessList().then(res => {\n        if (res.code == 1000) {\n          this.business = res.data;\n        }\n      });\n    },\n    changeProvince(val) {\n      this.cityFilter = [];\n      for (let i = 0; i < this.city.length; i++) {\n        let item = this.city[i];\n        if (item.province == val) {\n          this.cityFilter.push(item);\n        }\n      }\n    },\n    changeCity(val) {\n      this.businessFilter = [];\n      for (let i = 0; i < this.business.length; i++) {\n        let item = this.business[i];\n        if (item.city == val) {\n          this.businessFilter.push(item);\n        }\n      }\n    },\n    submit() {\n      this.$refs[\"ruleForm\"].validate(valid => {\n        if (valid) {\n          saveUser(this.form).then(res => {\n            if (res.code == 1000) {\n              this.$notify.success({\n                title: '成功',\n                message: \"保存成功\"\n              });\n              this.handleClose();\n            } else {\n              this.$notify.error({\n                title: '错误',\n                message: res.message\n              });\n            }\n          });\n        } else {\n          return false;\n        }\n      });\n    },\n    handleClose() {\n      this.form = {\n        idArrary: \"\",\n        deptId: \"\",\n        userName: \"\",\n        loginAccount: \"\",\n        userType: 1,\n        email: \"\",\n        tel: \"\",\n        sex: \"\",\n        avatar: \"\",\n        password: \"\",\n        status: \"0\",\n        remark: \"\",\n        roleIds: [],\n        coverImg: \"\",\n        name: \"\",\n        star: \"\",\n        openTime: \"\",\n        address: \"\",\n        average: \"\",\n        province: \"\",\n        city: \"\",\n        open: \"0\",\n        business: \"\",\n        introduce: \"\",\n        roleIds: ['fe8c3705cbd7b5046bf41ceae62e8640']\n      };\n      this.fileList = [];\n      this.$emit(\"addUserFalse\");\n    }\n  },\n  created() {},\n  mounted() {\n    this.getApeProvinceList();\n    this.getCityList();\n    this.getApeBusinessList();\n  }\n};","map":{"version":3,"names":["mixin","saveUser","getCityList","getApeProvinceList","getApeBusinessList","mixins