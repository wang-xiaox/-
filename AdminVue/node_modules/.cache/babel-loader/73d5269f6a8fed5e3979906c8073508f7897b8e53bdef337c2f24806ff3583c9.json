{"ast":null,"code":"import { getUserById, editUser } from '../../../api/api';\nexport default {\n  data() {\n    return {\n      formLabelWidth: '80px',\n      form: {\n        idArrary: \"\",\n        deptId: \"\",\n        userName: \"\",\n        loginAccount: \"\",\n        userType: 0,\n        email: \"\",\n        tel: \"\",\n        sex: \"\",\n        avatar: \"\",\n        password: \"\",\n        status: \"0\",\n        remark: \"\",\n        roleIds: []\n      },\n      rules: {\n        userName: [{\n          required: true,\n          message: '请输入用户昵称',\n          trigger: 'blur'\n        }],\n        loginAccount: [{\n          required: true,\n          message: '请输入登陆账号',\n          trigger: 'blur'\n        }],\n        password: [{\n          required: true,\n          message: '请输入登陆密码',\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n  props: ['updateUserVisible', 'updateId', 'data', 'post', 'role'],\n  methods: {\n    submit() {\n      editUser(this.form).then(res => {\n        if (res.code == 1000) {\n          this.$notify.success({\n            title: '成功',\n            message: \"保存成功\"\n          });\n          this.handleClose();\n        } else {\n          this.$notify.error({\n            title: '错误',\n            message: res.message\n          });\n        }\n      });\n    },\n    handleChange(value) {\n      this.form.idArrary = \"\";\n      this.form.deptId = \"\";\n      var length = value.length;\n      if (length > 0) {\n        this.form.idArrary = value.join(\",\");\n        this.form.deptId = value[length - 1];\n      }\n    },\n    handleClose() {\n      this.$emit(\"updateUserFalse\");\n    }\n  },\n  watch: {\n    updateId(newVal) {\n      getUserById({\n        id: newVal\n      }).then(res => {\n        if (res.code == 1000) {\n          this.form = res.data;\n          this.form.status = res.data.sex + \"\";\n          this.form.status = res.data.status + \"\";\n          if (this.form.idArrary != \"0\") {\n            this.parentId = this.form.idArrary.split(\",\");\n          }\n        } else {\n          this.$notify.error({\n            title: '错误',\n            message: res.message\n          });\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["getUserById","editUser","data","formLabelWidth","form","idArrary","deptId","userName","loginAccount","userType","email","tel","sex","avatar","password","status","remark","roleIds","rules","required","message","trigger","props","methods","submit","then","res","code","$notify","success","title","handleClose","error","handleChange","value","length","join","$emit","watch","updateId","newVal","id","parentId","split"],"sources":["src/components/system/user/updateUser.vue"],"sourcesContent":["<template>\r\n<div>\r\n  <el-dialog title=\"修改用户\" width=\"40%\" :destroy-on-close=\"true\" :visible.sync=\"updateUserVisible\" :before-close=\"handleClose\">\r\n    <el-form :model=\"form\" :rules=\"rules\" ref=\"ruleForm\">\r\n      <el-row :gutter=\"10\">\r\n        <el-col :xs=\"24\" :sm=\"24\" :md=\"12\" :lg=\"12\" :xl=\"12\">\r\n            <span class=\"search-title\">用户昵称:</span>\r\n            <el-form-item prop=\"userName\" style=\"margin-bottom:0\">\r\n              <el-input v-model=\"form.userName\" size=\"mini\" placeholder=\"请输入用户昵称\" autocomplete=\"off\"></el-input>\r\n            </el-form-item>\r\n        </el-col>\r\n        \r\n        <el-col :xs=\"24\" :sm=\"24\" :md=\"12\" :lg=\"12\" :xl=\"12\">\r\n            <span class=\"search-title\">手机号码:</span>\r\n            <el-form-item prop=\"tel\" style=\"margin-bottom:0\">\r\n              <el-input v-model=\"form.tel\" size=\"mini\" placeholder=\"请输入手机号码\" autocomplete=\"off\"></el-input>\r\n            </el-form-item>\r\n        </el-col>\r\n        <el-col :xs=\"24\" :sm=\"24\" :md=\"12\" :lg=\"12\" :xl=\"12\">\r\n            <span class=\"search-title\">&nbsp;&nbsp;&nbsp;&nbsp;邮箱:</span>\r\n            <el-form-item prop=\"email\" style=\"margin-bottom:0\">\r\n              <el-input v-model=\"form.email\" size=\"mini\" placeholder=\"请输入邮箱\" autocomplete=\"off\"></el-input>\r\n            </el-form-item>\r\n        </el-col>\r\n        <el-col :xs=\"24\" :sm=\"24\" :md=\"12\" :lg=\"12\" :xl=\"12\">\r\n            <span class=\"search-title\">登陆账号:</span>\r\n            <el-form-item prop=\"loginAccount\" style=\"margin-bottom:0\">\r\n              <el-input v-model=\"form.loginAccount\" size=\"mini\" placeholder=\"请输入登陆账号\" autocomplete=\"off\"></el-input>\r\n            </el-form-item>\r\n        </el-col>\r\n        <el-col :xs=\"24\" :sm=\"24\" :md=\"12\" :lg=\"12\" :xl=\"12\">\r\n            <span class=\"search-title\">用户性别:</span>\r\n            <el-form-item prop=\"sex\" style=\"margin-bottom:0\">\r\n              <el-select size=\"mini\" v-model=\"form.sex\" placeholder=\"请选择用户性别\">\r\n                <el-option label=\"男\" value=\"0\"></el-option>\r\n                <el-option label=\"女\" value=\"1\"></el-option>\r\n              </el-select>\r\n            </el-form-item>\r\n        </el-col>\r\n        <el-col :xs=\"24\" :sm=\"24\" :md=\"12\" :lg=\"12\" :xl=\"12\">\r\n            <span class=\"search-title\">岗位:</span>\r\n            <el-form-item prop=\"postId\" style=\"margin-bottom:0\">\r\n              <el-select size=\"mini\" v-model=\"form.postId\" placeholder=\"请选择岗位\">\r\n                <el-option v-for=\"(item,index) in post\" :key=\"index\" :label=\"item.postName\" :value=\"item.id\"></el-option>\r\n              </el-select>\r\n            </el-form-item>\r\n        </el-col>\r\n        <el-col :xs=\"24\" :sm=\"24\" :md=\"12\" :lg=\"12\" :xl=\"12\">\r\n            <span class=\"search-title\">归属部门:</span>\r\n            <el-form-item prop=\"deptId\" style=\"margin-bottom:0\">\r\n              <el-cascader\r\n                    @change=\"handleChange\"\r\n                    size=\"mini\"\r\n                    :options=\"data\"\r\n                    :props=\"{ checkStrictly: true,value:'id',label:'deptName',children:'children' }\"\r\n                    clearable>\r\n                </el-cascader>\r\n            </el-form-item>\r\n        </el-col>\r\n        <el-col :xs=\"24\" :sm=\"24\" :md=\"12\" :lg=\"12\" :xl=\"12\">\r\n            <span class=\"search-title\">角色:</span>\r\n            <el-form-item prop=\"roleIds\" style=\"margin-bottom:0\">\r\n              <el-select size=\"mini\" multiple  v-model=\"form.roleIds\" placeholder=\"请选择角色\">\r\n                <el-option v-for=\"(item,index) in role\" :label=\"item.roleName\" :value=\"item.id\"></el-option>\r\n              </el-select>\r\n            </el-form-item>\r\n        </el-col>\r\n        \r\n        <el-col :xs=\"24\" :sm=\"24\" :md=\"12\" :lg=\"12\" :xl=\"12\">\r\n            <span class=\"search-title\">状态:</span>\r\n             <el-form-item prop=\"name\" style=\"margin-bottom:0\">\r\n                <el-radio-group style=\"margin-top:1px;height:40px;display:flex;align-items: center\" v-model=\"form.status\">\r\n                  <el-radio label=\"0\">正常</el-radio>\r\n                  <el-radio label=\"1\">停用</el-radio>\r\n                </el-radio-group>\r\n            </el-form-item>\r\n        </el-col>\r\n        <el-col :xs=\"24\" :sm=\"24\" :md=\"24\" :lg=\"24\" :xl=\"24\">\r\n            <span class=\"search-title\">&nbsp;&nbsp;&nbsp;&nbsp;备注:</span>\r\n            <el-input type=\"textarea\" v-model=\"form.remark\"></el-input>\r\n        </el-col>\r\n      </el-row>\r\n    </el-form>\r\n    <div slot=\"footer\" class=\"dialog-footer\">\r\n      <el-button size=\"mini\" type=\"primary\" @click=\"submit\">确 定</el-button>\r\n      <el-button size=\"mini\" @click=\"handleClose\">取 消</el-button>\r\n    </div>\r\n  </el-dialog>\r\n</div>\r\n</template>\r\n\r\n<script>\r\n  import {getUserById,editUser} from '../../../api/api'\r\n  export default {\r\n    data() {\r\n      return{\r\n        formLabelWidth: '80px',\r\n        form: {\r\n          idArrary: \"\",\r\n          deptId: \"\",\r\n          userName: \"\",\r\n          loginAccount: \"\",\r\n          userType: 0,\r\n          email: \"\",\r\n          tel: \"\",\r\n          sex: \"\",\r\n          avatar: \"\",\r\n          password: \"\",\r\n          status: \"0\",\r\n          remark: \"\",\r\n          roleIds: []\r\n        },\r\n        rules: {\r\n          userName: [\r\n            { required: true, message: '请输入用户昵称', trigger: 'blur' },\r\n          ],\r\n          loginAccount: [\r\n            { required: true, message: '请输入登陆账号', trigger: 'blur' },\r\n          ],\r\n          password: [\r\n            { required: true, message: '请输入登陆密码', trigger: 'blur' },\r\n          ],\r\n        }\r\n      }\r\n    },\r\n    props: ['updateUserVisible','updateId','data','post','role'],\r\n    methods: {\r\n      submit() {\r\n        editUser(this.form).then(res => {\r\n          if(res.code == 1000) {\r\n            this.$notify.success({\r\n                  title: '成功',\r\n                  message: \"保存成功\"\r\n                });\r\n            this.handleClose()\r\n          } else {\r\n            this.$notify.error({\r\n              title: '错误',\r\n              message: res.message\r\n            });\r\n          }\r\n        })\r\n      },\r\n      handleChange(value) {\r\n        this.form.idArrary = \"\"\r\n        this.form.deptId = \"\"\r\n        var length = value.length\r\n        if(length > 0) {\r\n          this.form.idArrary = value.join(\",\")\r\n          this.form.deptId = value[length-1]\r\n        }\r\n      },\r\n      handleClose() {\r\n        this.$emit(\"updateUserFalse\")\r\n      }\r\n    },\r\n    watch: {\r\n      updateId(newVal) {\r\n        getUserById({id:newVal}).then(res => {\r\n          if(res.code == 1000) {\r\n            this.form = res.data\r\n            this.form.status = res.data.sex + \"\"\r\n            this.form.status = res.data.status + \"\"\r\n            if (this.form.idArrary != \"0\") {\r\n              this.parentId = this.form.idArrary.split(\",\")\r\n            }\r\n          } else {\r\n            this.$notify.error({\r\n              title: '错误',\r\n              message: res.message\r\n            });\r\n          }\r\n        })\r\n      }\r\n    }\r\n }\r\n</script>\r\n\r\n<style lang=scss scoped>\r\n  .el-col {\r\n      display: flex;\r\n      flex-direction: row;\r\n      align-items: center;\r\n  }\r\n  .search-title {\r\n      font-family: '黑体';\r\n      float: right;\r\n      white-space: nowrap;\r\n      font-size: 14px;\r\n      width: 63px;\r\n      text-align: right;\r\n  }\r\n</style>"],"mappings":"AA4FA,SAAAA,WAAA,EAAAC,QAAA;AACA;EACAC,KAAA;IACA;MACAC,cAAA;MACAC,IAAA;QACAC,QAAA;QACAC,MAAA;QACAC,QAAA;QACAC,YAAA;QACAC,QAAA;QACAC,KAAA;QACAC,GAAA;QACAC,GAAA;QACAC,MAAA;QACAC,QAAA;QACAC,MAAA;QACAC,MAAA;QACAC,OAAA;MACA;MACAC,KAAA;QACAX,QAAA,GACA;UAAAY,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,EACA;QACAb,YAAA,GACA;UAAAW,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,EACA;QACAP,QAAA,GACA;UAAAK,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;MAEA;IACA;EACA;EACAC,KAAA;EACAC,OAAA;IACAC,OAAA;MACAvB,QAAA,MAAAG,IAAA,EAAAqB,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAC,OAAA,CAAAC,OAAA;YACAC,KAAA;YACAV,OAAA;UACA;UACA,KAAAW,WAAA;QACA;UACA,KAAAH,OAAA,CAAAI,KAAA;YACAF,KAAA;YACAV,OAAA,EAAAM,GAAA,CAAAN;UACA;QACA;MACA;IACA;IACAa,aAAAC,KAAA;MACA,KAAA9B,IAAA,CAAAC,QAAA;MACA,KAAAD,IAAA,CAAAE,MAAA;MACA,IAAA6B,MAAA,GAAAD,KAAA,CAAAC,MAAA;MACA,IAAAA,MAAA;QACA,KAAA/B,IAAA,CAAAC,QAAA,GAAA6B,KAAA,CAAAE,IAAA;QACA,KAAAhC,IAAA,CAAAE,MAAA,GAAA4B,KAAA,CAAAC,MAAA;MACA;IACA;IACAJ,YAAA;MACA,KAAAM,KAAA;IACA;EACA;EACAC,KAAA;IACAC,SAAAC,MAAA;MACAxC,WAAA;QAAAyC,EAAA,EAAAD;MAAA,GAAAf,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAvB,IAAA,GAAAsB,GAAA,CAAAxB,IAAA;UACA,KAAAE,IAAA,CAAAW,MAAA,GAAAW,GAAA,CAAAxB,IAAA,CAAAU,GAAA;UACA,KAAAR,IAAA,CAAAW,MAAA,GAAAW,GAAA,CAAAxB,IAAA,CAAAa,MAAA;UACA,SAAAX,IAAA,CAAAC,QAAA;YACA,KAAAqC,QAAA,QAAAtC,IAAA,CAAAC,QAAA,CAAAsC,KAAA;UACA;QACA;UACA,KAAAf,OAAA,CAAAI,KAAA;YACAF,KAAA;YACAV,OAAA,EAAA