ateParamVisible = true;\n    },\n    updateParamBtn() {\n      this.updateParam(this.update[0]);\n    },\n    deleteDateBtn() {\n      this.$confirm('确定删除选中的' + this.remove.length + '条数据?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        this.deleteDate(this.remove.join(\",\"));\n      }).catch(() => {});\n    },\n    deleteDate(ids) {\n      removeParam({\n        ids: ids\n      }).then(res => {\n        if (res.code == 1000) {\n          this.$message({\n            type: 'success',\n            message: '删除成功!'\n          });\n          this.query();\n        } else {\n          this.$notify.error({\n            title: '错误',\n            message: res.message\n          });\n        }\n      });\n    }\n  },\n  mounted() {\n    this.query();\n  }\n};","map":{"version":3,"names":["getParamPage","removeParam","addParam","updateParam","data","loading","update","remove","updateId","addParamVisible","updateParamVisible","search","paramName","paramKey","within","pageNumber","pageSize","total","tableData","components","methods","query","then","res","code","records","$notify","error","title","message","refresh","handleCurrentChange","val","handleSizeChange","handleSelectionChange","i","length","item","push","id","addParamFalse","updateParamFalse","updateParamBtn","deleteDateBtn","$confirm","confirmButtonText","cancelButtonText","type","deleteDate","join","catch","ids","$message","mounted"],"sources":["src/views/system/param/param.vue"],"sourcesContent":["<template>\r\n  <div class=\"param\">\r\n      <div class=\"search-table\">\r\n        <div class=\"search\">\r\n            <el-row :gutter=\"10\" style=\"padding:10px\">\r\n                <el-col :xs=\"24\" :sm=\"12\" :md=\"8\" :lg=\"8\" :xl=\"8\">\r\n                    <span class=\"search-title\">参数名称:</span>\r\n                    <el-input\r\n                        style=\"margin-top:10px\"\r\n                        size=\"mini\