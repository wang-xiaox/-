ame == \"admin\" && this.password == \"admin\") {\r\n                this.$notify({\r\n                    title: '成功',\r\n                    message: '登陆成功',\r\n                    type: 'success'\r\n                });\r\n            } else {\r\n                this.$notify.error({\r\n                    title: '失败',\r\n                    message: '用户名或密码错误'\r\n                });\r\n            }\r\n        }\r\n    },\r\n    created() {\r\n     \r\n    },\r\n    mounted() {\r\n      \r\n    }\r\n }\r\n</script>\r\n\r\n<style lang=scss scoped>\r\n  .login {\r\n    width:100%;\r\n    height:100%;\r\n    background-color: #1F2029;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    flex-direction: column;\r\n  }\r\n  .title {\r\n      height: 10%;\r\n      display: flex;\r\n      align-items: center;\r\n      font-size: 20px;\r\n      color: #fff;\r\n      font-weight: bold;\r\n  }\r\n  .login-card {\r\n      width: 30%;\r\n      height: 55%;\r\n      background-color: #292A37;\r\n      background-image: url(../../assets/image/pat.svg);\r\n      display: flex;\r\n      flex-direction: column;\r\n      align-items: center;\r\n  }\r\n  .login-title {\r\n      height:30%;\r\n      color:#C4C3CA;\r\n      font-weight:bold;\r\n      font-size:25px;\r\n      display:flex;\r\n      align-items: center\r\n  }\r\n  .login-input {\r\n      width:80%;height:25%\r\n  }\r\n  .forget {\r\n      margin-top: 6%;\r\n      color:#C4C3CA;font-weight:bold;font-size:15px;\r\n  }\r\n  .forget:hover {\r\n      color: #FDE9A6;\r\n  }\r\n  .login-btn {\r\n      margin-top: 6%;\r\n      width:120px;\r\n      height: 46px;\r\n      background-color: #ffeba7;\r\n      font-weight: bold;\r\n      font-family: '黑体';\r\n      font-size: 17px;\r\n  }\r\n  .login-btn:hover {\r\n      background-color: #102770;\r\n      color: #FDE9A6;\r\n      font-weight: bold;\r\n      font-family: '黑体';\r\n      font-size: 17px;\r\n      border: none;\r\n  }\r\n  .el-input