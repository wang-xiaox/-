r\n      handleChange(value) {\r\n        this.form.idArrary = \"\"\r\n        var length = value.length\r\n        if(length > 0) {\r\n          this.form.idArrary = value.join(\",\")\r\n          this.form.parentId = value[length-1]\r\n        }\r\n      }\r\n    },\r\n    watch: {\r\n      updateId(newVal) {\r\n        if(newVal) {\r\n          getById({id:newVal}).then(res => {\r\n            if(res.code == 1000) {\r\n              this.form = res.data\r\n              this.form.status = res.data.status + \"\"\r\n              if (this.form.idArrary != \"0\") {\r\n                this.parentId = this.form.idArrary.split(\",\")\r\n              }\r\n            } else {\r\n              this.$notify.error({\r\n                title: '错误',\r\n                message: res.message\r\n              });\r\n            }\r\n          })\r\n        }\r\n      }\r\n    }\r\n }\r\n</script>\r\n\r\n<style scoped>\r\n  .el-col {\r\n      display: flex;\r\n      flex-direction: row;\r\n      align-items: center;\r\n      margin-top: 12px\r\n  }\r\n  .search-title {\r\n      font-family: '黑体';\r\n      float: right;\r\n      white-space: nowrap;\r\n      font-size: 14px;\r\n      width: 84px;\r\n      text-align: right;\r\n  }\r\n  .el-tree {\r\n      border: 1px solid #BDC1C9;\r\n  }\r\n</style>"],"mappings":"AA4EA,SAAAA,OAAA,EAAAC,QAAA;AACA;EACAC,KAAA;IACA;MACAC,cAAA;MACAC,IAAA;QACAC,QAAA;QACAC,QAAA;QACAC,QAAA;QACAC,MAAA;QACAC,KAAA;QACAC,KAAA;QACAC,MAAA;QACAC,QAAA;MACA;MACAP,QAAA;MACAQ,KAAA;QACAP,QAAA,GACA;UAAAQ,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,EACA;QACAR,MAAA,GACA;UAAAM,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,EACA;QACAP,KAAA,GACA;UAAAK,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,EACA;QACAN,KAAA,GACA;UAAAI,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;MAEA;IACA;EACA;EACAC,KAAA;EACAC,OAAA;IACAC,OAAA;MACAlB,QAAA,MAAAG,IAAA,EAAAgB,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAC,OAAA,CAAAC,OAAA;YACAC,KAAA;YACAV,OAAA;UACA;UACA,KAAAW,WAAA;QACA;UACA,KAAAH,OAAA,CAAAI,KAAA;YACAF,KAAA;YACAV,OAAA,EAAAM,GAAA,CAAAN