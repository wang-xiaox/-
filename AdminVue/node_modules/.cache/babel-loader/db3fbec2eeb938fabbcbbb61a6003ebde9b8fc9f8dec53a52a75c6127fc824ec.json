hen(res => {\n        if (res.code == 1000) {\n          this.$notify.success({\n            title: '成功',\n            message: \"保存成功\"\n          });\n          this.handleClose();\n        } else {\n          this.$notify.error({\n            title: '错误',\n            message: res.message\n          });\n        }\n      });\n    },\n    handleClose() {\n      this.fileList = [];\n      this.$emit(\"updateUserFalse\");\n    }\n  },\n  mounted() {\n    this.getApeMajorList();\n    this.getApeSchoolList();\n  },\n  watch: {\n    updateId(newVal) {\n      if (newVal) {\n        getUserById({\n          id: newVal\n        }).then(res => {\n          if (res.code == 1000) {\n            this.form = res.data;\n            this.form.sex = res.data.sex + \"\";\n            this.form.status = res.data.status + \"\";\n            this.fileList.push({\n              url: this.$store.state.configure.HOST + this.form.flair\n            });\n          } else {\n            this.$notify.error({\n              title: '错误',\n              message: res.message\n            });\n          }\n        });\n      }\n    }\n  }\n};","map":{"version":3,"names":["mixin","getUserById","editUser","getApeSchoolList","getApeMajorList","mixins","data","formLabelWidth","school","major","dialogImageUrl","dialogVisible","form","userName","loginAccount","email","tel","sex","password","status","userType","country","agree","age","birth","flair","address","workDate","remark","fileList","rules","required","message","trigger","props","methods","handleAvatorSuccess","res","_this","code","$message","type","push","url","$store","state","configure","HOST","$notify","error","title","handleRemove","file","handlePictureCardPreview","then","submit","success","handleClose","$emit","mounted","watch","updateId","newVal","id"],"sources":["src/components/system/teacher/updateTeacher.vue"],"sourcesContent":["<template>\r\n<div>\r\n  <el-dialog title=\"编辑教练\" width=\"40%\" :destroy-on-close=\"true\" :visible.sync=\