        email: \"\",\r\n          tel: \"\",\r\n          sex: \"\",\r\n          avatar: \"\",\r\n          password: \"\",\r\n          status: \"0\",\r\n          remark: \"\",\r\n          roleIds: []\r\n        }\r\n        this.$emit(\"addUserFalse\")\r\n      },\r\n      handleChange(value) {\r\n        this.form.idArrary = \"\"\r\n        this.form.deptId = \"\"\r\n        var length = value.length\r\n        if(length > 0) {\r\n          this.form.idArrary = value.join(\",\")\r\n          this.form.deptId = value[length-1]\r\n        }\r\n      }\r\n    },\r\n    created() {\r\n     \r\n    },\r\n    mounted() {\r\n      \r\n    }\r\n }\r\n</script>\r\n\r\n<style   scoped>\r\n  .el-col {\r\n      display: flex;\r\n      flex-direction: row;\r\n      align-items: center;\r\n      margin-top: 12px\r\n  }\r\n  .search-title {\r\n      font-family: '黑体';\r\n      float: right;\r\n      white-space: nowrap;\r\n      font-size: 14px;\r\n      width: 63px;\r\n      text-align: right;\r\n  }\r\n</style>"],"mappings":"AAiGA,SAAAA,QAAA;AACA;EACAC,KAAA;IACA;MACAC,cAAA;MACAC,IAAA;QACAC,QAAA;QACAC,MAAA;QACAC,QAAA;QACAC,YAAA;QACAC,QAAA;QACAC,KAAA;QACAC,GAAA;QACAC,GAAA;QACAC,MAAA;QACAC,QAAA;QACAC,MAAA;QACAC,MAAA;QACAC,OAAA;MACA;MACAC,KAAA;QACAX,QAAA,GACA;UAAAY,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,EACA;QACAb,YAAA,GACA;UAAAW,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,EACA;QACAP,QAAA,GACA;UAAAK,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;MAEA;IACA;EACA;EACAC,KAAA;EACAC,OAAA;IACAC,OAAA;MACA,KAAAC,KAAA,aAAAC,QAAA,CAAAC,KAAA;QACA,IAAAA,KAAA;UACA1B,QAAA,MAAAG,IAAA,EAAAwB,IAAA,CAAAC,GAAA;YACA,IAAAA,GAAA,CAAAC,IAAA;cACA,KAAAC,OAAA,CAAAC,OAAA;gBACAC,KAAA;gBACAb,OAAA;cACA;cACA,KAAAc,WAAA;YACA;cACA,KAAAH,OAAA,CAAAI,KAAA;gBACAF,KAAA;gBACAb,OAAA,EAAAS,GAAA,CAAAT;cACA;YACA;UACA;QACA;UACA;QACA;MACA;IACA;IACAc,YAAA;MACA,KAAA9B,IAAA;QACAC,QAAA;QACAC,MAAA;QACAC,QAAA;QACAC,YAAA;QACAC,QAAA;QACAC,KAAA;QACAC,GAAA;QACAC,GAAA;QACAC,MAAA;QACAC,QAAA;QACAC,MAAA;QACAC,MAAA;QACAC,OAAA;MACA;MACA,KAAAmB,KAAA;IACA;IACAC,aAAAC,KAAA;