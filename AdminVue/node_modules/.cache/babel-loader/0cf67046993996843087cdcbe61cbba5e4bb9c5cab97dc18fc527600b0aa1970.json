       this.$notify.error({\n              title: '错误',\n              message: res.message\n            });\n          }\n        });\n      }\n    }\n  }\n};","map":{"version":3,"names":["getUserById","editUser","data","formLabelWidth","form","userName","loginAccount","userType","email","tel","sex","avatar","password","status","remark","rules","required","message","trigger","props","methods","submit","then","res","code","$notify","success","title","handleClose","error","$emit","watch","updateId","newVal","id"],"sources":["src/components/system/admin/updateAdmin.vue"],"sourcesContent":["<template>\r\n<div>\r\n  <el-dialog title=\"修改管理员\" width=\"40%\" :destroy-on-close=\"true\" :visible.sync=\"updateUserVisible\" :before-close=\"handleClose\">\r\n    <el-form :model=\"form\" :rules=\"rules\" ref=\"ruleForm\">\r\n      <el-row :gutter=\"10\">\r\n        <el-col :xs=\"24\" :sm=\"24\" :md=\"24\" :lg=\"24\" :xl=\"24\">\r\n            <span class=\"search-title\">用户昵称:</span>\r\n            <div style=\"width:100%\">\r\n              <el-form-item prop=\"userName\" style=\"margin-bottom:0\">\r\n                <el-input v-model=\"form.userName\" size=\"mini\" placeholder=\"请输入用户昵称\" autocomplete=\"off\"></el-input>\r\n              </el-form-item>\r\n            </div>\r\n        </el-col>\r\n        \r\n        <el-col :xs=\"24\" :sm=\"24\" :md=\"24\" :lg=\"24\" :xl=\"24\">\r\n            <span class=\"search-title\">手机号码:</span>\r\n            <div style=\"width:100%\">\r\n              <el-form-item prop=\"tel\" style=\"margin-bottom:0\">\r\n                <el-input v-model=\"form.tel\" size=\"mini\" placeholder=\"请输入手机号码\" autocomplete=\"off\"></el-input>\r\n              </el-form-item>\r\n            </div>\r\n        </el-col>\r\n        <el-col :xs=\"24\" :sm=\"24\" :md=\"24\" :lg=\"24\" :xl=\"24\">\r\n            <span class=\"search-title\">&nbsp;&nbsp;&nbsp;&nbsp;邮箱:</span>\r\n            <div style=\"width:100%\">\r\n              <el-form-item prop=\"email