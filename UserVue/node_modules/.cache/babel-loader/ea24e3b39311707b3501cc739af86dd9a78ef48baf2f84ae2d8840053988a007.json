{"ast":null,"code":"import { saveMenu } from '../../../api/api';\nexport default {\n  data() {\n    return {\n      formLabelWidth: '80px',\n      form: {\n        idArrary: \"\",\n        menuName: \"\",\n        parentId: \"\",\n        orderNum: 1,\n        target: \"0\",\n        routeUrl: \"\",\n        componentUrl: \"\",\n        param: \"\",\n        menuType: \"0\",\n        visible: \"0\",\n        status: \"0\",\n        perms: \"\",\n        icon: \"\"\n      },\n      rules: {\n        menuName: [{\n          required: true,\n          message: '请输入菜单名称',\n          trigger: 'blur'\n        }],\n        routeUrl: [{\n          required: true,\n          message: '请输入路由地址',\n          trigger: 'blur'\n        }],\n        componentUrl: [{\n          required: true,\n          message: '请输入组件地址',\n          trigger: 'blur'\n        }],\n        param: [{\n          required: true,\n          message: '请输入菜单参数',\n          trigger: 'blur'\n        }],\n        perms: [{\n          required: true,\n          message: '请输入权限标识',\n          trigger: 'blur'\n        }]\n      },\n      restaurants: [{\n        \"value\": \"el-icon-platform-eleme\"\n      }, {\n        \"value\": \"el-icon-eleme\"\n      }, {\n        \"value\": \"el-icon-delete-solid\"\n      }, {\n        \"value\": \"el-icon-delete\"\n      }, {\n        \"value\": \"el-icon-s-tools\"\n      }, {\n        \"value\": \"el-icon-setting\"\n      }, {\n        \"value\": \"el-icon-user-solid\"\n      }, {\n        \"value\": \"el-icon-user\"\n      }, {\n        \"value\": \"el-icon-phone\"\n      }, {\n        \"value\": \"el-icon-phone-outline\"\n      }, {\n        \"value\": \"el-icon-more\"\n      }, {\n        \"value\": \"el-icon-more-outline\"\n      }, {\n        \"value\": \"el-icon-star-on\"\n      }, {\n        \"value\": \"el-icon-star-off\"\n      }, {\n        \"value\": \"el-icon-s-goods\"\n      }, {\n        \"value\": \"el-icon-goods\"\n      }, {\n        \"value\": \"el-icon